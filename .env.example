# API Keys
OPENAI_API_KEY=your_openai_api_key_here

# Model Configuration
TEXT_DETECTION_MODEL=microsoft/deberta-v3-base
ZERO_SHOT_MODEL=facebook/bart-large-mnli
IMAGE_DETECTION_MODEL=stylegan-detector
VIDEO_DETECTION_MODEL=deepfake-detector

# Confidence Thresholds
TEXT_CONFIDENCE_THRESHOLD=0.7
IMAGE_CONFIDENCE_THRESHOLD=0.8
VIDEO_CONFIDENCE_THRESHOLD=0.75

# API Settings
MAX_TOKENS=1000
TEMPERATURE=0.7
MODEL_NAME=gpt-3.5-turbo

# Rate Limiting
RATE_LIMIT_REQUESTS=100
RATE_LIMIT_PERIOD=3600

# Logging
LOG_LEVEL=INFO
LOG_FILE=app.log

# Security
SECRET_KEY=your_secret_key_here
ALGORITHM=HS256
ACCESS_TOKEN_EXPIRE_MINUTES=30

# Database (if needed later)
DATABASE_URL=your_database_url_here

# Cache Configuration
CACHE_TTL=3600 